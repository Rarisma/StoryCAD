      <StoryNode UUID="3F55D0FD-F522-4884-96E3-3EFA667AA882" Type="Folder" Name="Problems" IsExpanded="True" IsSelected="False" IsRoot="False">
        <StoryNode UUID="0D9D7606-EA82-460B-81AA-7C9ED1B01DAD" Type="Problem" Name="The old man hasn't cought a fish in 84 days" IsExpanded="False" IsSelected="False" IsRoot="False" />
        <StoryNode UUID="B875453F-EDB4-4E17-AA39-AE9855ED9C08" Type="Problem" Name="Santiago hooks a marlin bigger than his boat" IsExpanded="False" IsSelected="False" IsRoot="False" />
        <StoryNode UUID="8485FC3F-69EF-43B6-966A-538DAB35F43B" Type="Problem" Name="Sharks are attracted to the fish" IsExpanded="False" IsSelected="False" IsRoot="False" />
        <StoryNode UUID="51F3F68E-4A44-4F06-8515-A3C1751C0709" Type="Problem" Name="Manolin must abandon his jinxed mentor" IsExpanded="False" IsSelected="False" IsRoot="False" />
      </StoryNode>
