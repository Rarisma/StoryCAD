<ContentDialog
    x:Class="StoryBuilder.Services.Dialogs.Tools.ConflictDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="Conflict Builder"
    PrimaryButtonText="Copy to Protagonist"
    SecondaryButtonText="Copy to Antagonist"
    CloseButtonText="Cancel"
    xmlns:usercontrols="using:StoryBuilder.Controls">
  
    <Grid Padding="14,0,14,0">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <usercontrols:Conflict x:Name="Conflict" x:FieldModifier="public" Grid.Row="4" MinWidth="400" />
    </Grid>
</ContentDialog>
